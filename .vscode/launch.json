  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'log-plot'",
      "cargo": {
        "args": ["build", "--bin=log-plot", "--package=log-plot"],
        "filter": {
          "name": "log-plot",
          "kind": "bin"
        }
      },
      "args": [
        "-f",
        "test_files/c.gcode",
        "-d",
        "X([0-9.]*)",
        "-i",
        "G1",
        "-g"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'log-plot' 1",
      "cargo": {
        "args": ["build", "--bin=log-plot", "--package=log-plot"],
        "filter": {
          "name": "log-plot",
          "kind": "bin"
        }
      },
      "args": [
        "-f",
        "test_files/c.gcode",
        "-d",
        "(?<X>(?<=X)[0-9.]*)",
        "-d",
        "(?<Y>(?<=Y)[0-9.]*)",
        "-d",
        "(?<=Z)[0-9.]*",
        "-i",
        "G1"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'log-plot'",
      "cargo": {
        "args": ["test", "--no-run", "--bin=log-plot", "--package=log-plot"],
        "filter": {
          "name": "log-plot",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
